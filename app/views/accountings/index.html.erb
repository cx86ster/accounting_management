<p id="notice"><%= notice %></p>

<h1>Listing Accountings</h1>

<table>
  <thead>
    <tr>
      <th>Accounting date</th>
      <th>Accounting money</th>
      <th>Item name</th>
      <th>Accounting group</th>
      <th>Purchase place</th>
      <th>Accounting source</th>
      <th>Accounting class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accountings.each do |accounting| %>
      <tr>
        <td><%= accounting.accounting_date %></td>
        <td><%= accounting.accounting_money %></td>
        <td><%= accounting.item_name %></td>
        <td><%= accounting.accounting_group_id %></td>
        <td><%= accounting.purchase_place %></td>
        <td><%= accounting.accounting_source %></td>
        <td><%= accounting.accounting_class %></td>
        <td><%= link_to 'Show', accounting %></td>
        <td><%= link_to 'Edit', edit_accounting_path(accounting) %></td>
        <td><%= link_to 'Destroy', accounting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accounting', new_accounting_path %>
